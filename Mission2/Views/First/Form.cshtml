@{
    ViewData["Title"] = "Mission 3 Form";
}

@model FormResponseModel;

<!--Form-->
<h2>Grade Calculator</h2>
<form  method="post">
    <label>Assignments:</label>
    <input id="assignments" asp-for="Assignment" /><br />
    <label>Group Project:</label>
    <input id="groups"  asp-for="Group" /><br />
    <label>Quizzes:</label>
    <input id="quizzes" asp-for="Quiz" /><br />
    <label>Exams:</label>
    <input id="exams" asp-for="Exam"/><br />
    <label>INTEX:</label>
    <input id="intex"  asp-for="Intex"/><br />
    <input id="calc" type="submit" value="Calculate" />
</form>

<div asp-validation-summary="All"></div>
<!--Javascript-->
<script src="~/js/script.js"></script>